filters:
  and:
    - categories.contains(link("Board Game"))
    - '!file.path.startsWith("_obsidian")'
properties:
  file.name:
    displayName: Game
  note.rating:
    displayName: Rating
  note.last:
    displayName: Last
views:
  - type: table
    name: Table
    order:
      - file.name
      - rating
      - last
