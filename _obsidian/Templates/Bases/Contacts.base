filters:
  and:
    - categories.contains(link("Person"))
    - type.contains(link("Contact"))
    - '!file.path.startsWith("_obsidian")'
properties:
  file.name:
    displayName: Name
  note.email:
    displayName: Email
  note.phone:
    displayName: Phone
  note.created:
    displayName: Created
views:
  - type: table
    name: All
    order:
      - file.name
      - email
      - phone
      - created
